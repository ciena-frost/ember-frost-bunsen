    "ember-cli-babel": "^5.1.8",
    "ember-cli-htmlbars": "^1.0.3"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "ember-frost-icon-pack": {
    "name": "frost-bunsen"
  },
  "ember-lodash-shim": {
    "includes": [
      "cloneDeep",
      "isEqual",
      "isPlainObject"
