{{#if showSection}}
  {{#frost-bunsen-section
    collapsible=mergedConfig.collapsible
    description=mergedConfig.description
    required=isRequired
    title=mergedConfig.label
  }}
    {{#if mergedConfig.model}}
      {{#if isSubModelObject}}
        {{frost-bunsen-cell
          bunsenId=renderId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=mergedConfig
          errors=errors
          formDisabled=formDisabled
          formHook=formHook
          onChange=onChange
          onError=onError
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          showAllErrors=showAllErrors
          value=value
        }}
      {{else if isSubModelArray}}
        {{#if isArrayItem}}
          {{frost-bunsen-array-inline-item
            bunsenId=nonIndexId
            bunsenModel=subModel
            bunsenView=bunsenView
            cellConfig=mergedConfig
            errors=errors
            formDisabled=formDisabled
            formHook=formHook
            index=index
            onChange=onChange
            onError=onError
            readOnly=readOnly
            registerForFormValueChanges=registerForFormValueChanges
            renderers=renderers
            showAllErrors=showAllErrors
            showRemoveButton=false
            sortable=false
            value=value
          }}
        {{else}}
          {{frost-bunsen-array-container
            bunsenId=renderId
            bunsenModel=subModel
            bunsenView=bunsenView
            cellConfig=mergedConfig
            errors=errors
            formDisabled=formDisabled
            formHook=formHook
            onChange=onChange
            onError=onError
            readOnly=readOnly
            registerForFormValueChanges=registerForFormValueChanges
            renderers=renderers
            required=required
            showAllErrors=showAllErrors
            value=value
          }}
        {{/if}}
      {{else}}
        {{frost-bunsen-input-wrapper
          bunsenId=renderId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=mergedConfig
          errorMessage=errorMessage
          formDisabled=formDisabled
          formHook=formHook
          onChange=onChange
          onError=onError
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          required=required
          showAllErrors=showAllErrors
          isDependencyMet=isDependencyMet
          value=renderValue
        }}
      {{/if}}
    {{/if}}
    {{#each mergedConfig.children as |child index|}}
      {{frost-bunsen-cell
        bunsenId=bunsenId
        bunsenModel=bunsenModel
        bunsenView=bunsenView
        cellConfig=child
        errors=errors
        formDisabled=formDisabled
        formHook=formHook
        onChange=onChange
        onError=onError
        readOnly=readOnly
        registerForFormValueChanges=registerForFormValueChanges
        renderers=renderers
        showAllErrors=showAllErrors
        value=value
      }}
    {{/each}}
  {{/frost-bunsen-section}}
{{else}}
  {{#if mergedConfig.model}}
    {{#if isSubModelObject}}
      {{frost-bunsen-cell
        bunsenId=renderId
        bunsenModel=subModel
        bunsenView=bunsenView
        cellConfig=mergedConfig
        errors=errors
        formDisabled=formDisabled
        formHook=formHook
        onChange=onChange
        onError=onError
        readOnly=readOnly
        registerForFormValueChanges=registerForFormValueChanges
        renderers=renderers
        showAllErrors=showAllErrors
        value=value
      }}
    {{else if isSubModelArray}}
      {{#if isArrayItem}}
        {{frost-bunsen-array-inline-item
          bunsenId=nonIndexId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=mergedConfig
          errors=errors
          formDisabled=formDisabled
          formHook=formHook
          index=index
          onChange=onChange
          onError=onError
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          showAllErrors=showAllErrors
          showRemoveButton=false
          sortable=false
          value=value
        }}
      {{else}}
        {{frost-bunsen-array-container
          bunsenId=renderId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=mergedConfig
          errors=errors
          formDisabled=formDisabled
          formHook=formHook
          onChange=onChange
          onError=onError
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          required=required
          showAllErrors=showAllErrors
          value=value
        }}
      {{/if}}
    {{else}}
      {{frost-bunsen-input-wrapper
        bunsenId=renderId
        bunsenModel=subModel
        bunsenView=bunsenView
        cellConfig=mergedConfig
        errorMessage=errorMessage
        formDisabled=formDisabled
        formHook=formHook
        onChange=onChange
        onError=onError
        readOnly=readOnly
        registerForFormValueChanges=registerForFormValueChanges
        renderers=renderers
        required=required
        showAllErrors=showAllErrors
        isDependencyMet=isDependencyMet
        value=renderValue
      }}
    {{/if}}
  {{/if}}
  {{#each mergedConfig.children as |child index|}}
    {{frost-bunsen-cell
      bunsenId=bunsenId
      bunsenModel=bunsenModel
      bunsenView=bunsenView
      cellConfig=child
      errors=errors
      formDisabled=formDisabled
      formHook=formHook
      onChange=onChange
      onError=onError
      readOnly=readOnly
      registerForFormValueChanges=registerForFormValueChanges
      renderers=renderers
      showAllErrors=showAllErrors
      value=value
    }}
  {{/each}}
{{/if}}
