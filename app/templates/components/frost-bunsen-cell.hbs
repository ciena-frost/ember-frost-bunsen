{{#if (and mergedConfig.label mergedCell.children)}}
  {{#frost-bunsen-section
    collapsible=mergedConfig.collapsible
    description=mergedConfig.description
    required=isRequired
    title=mergedConfig.label
  }}
    {{#if mergedConfig.model}}
      {{#if isSubModelObject}}
        {{frost-bunsen-cell
          bunsenId=renderId
          bunsenModel=subModel
          bunsenStore=bunsenStore
          cellConfig=mergedConfig
          errors=errors
          onChange=onChange
          readOnly=readOnly
          value=value
        }}
      {{else if isSubModelArray}}
        {{#if isArrayItem}}
          {{frost-bunsen-array-inline-item
            bunsenId=nonIndexId
            bunsenModel=subModel
            bunsenStore=bunsenStore
            cellConfig=mergedConfig
            errors=errors
            index=index
            onChange=onChange
            readOnly=readOnly
            showRemoveButton=false
            sortable=false
            value=value
          }}
        {{else}}
          {{frost-bunsen-array-container
            bunsenId=renderId
            bunsenModel=subModel
            bunsenStore=bunsenStore
            cellConfig=mergedConfig
            errors=errors
            onChange=onChange
            readOnly=readOnly
            required=required
            value=value
          }}
        {{/if}}
      {{else}}
        {{frost-bunsen-input-wrapper
          bunsenId=renderId
          bunsenModel=subModel
          bunsenStore=bunsenStore
          cellConfig=mergedConfig
          errorMessage=errorMessage
          onChange=onChange
          readOnly=readOnly
          required=required
          isDependencyMet=isDependencyMet
          value=renderValue
        }}
      {{/if}}
    {{/if}}
    {{#each mergedConfig.children as |child index|}}
      {{frost-bunsen-cell
        bunsenId=bunsenId
        bunsenModel=bunsenModel
        bunsenStore=bunsenStore
        cellConfig=child
        errors=errors
        onChange=onChange
        readOnly=readOnly
        value=value
      }}
    {{/each}}
  {{/frost-bunsen-section}}
{{else}}
  {{#if mergedConfig.model}}
    {{#if isSubModelObject}}
      {{frost-bunsen-cell
        bunsenId=renderId
        bunsenModel=subModel
        bunsenStore=bunsenStore
        cellConfig=mergedConfig
        errors=errors
        onChange=onChange
        readOnly=readOnly
        value=value
      }}
    {{else if isSubModelArray}}
      {{#if isArrayItem}}
        {{frost-bunsen-array-inline-item
          bunsenId=nonIndexId
          bunsenModel=subModel
          bunsenStore=bunsenStore
          cellConfig=mergedConfig
          errors=errors
          index=index
          onChange=onChange
          readOnly=readOnly
          showRemoveButton=false
          sortable=false
          value=value
        }}
      {{else}}
        {{frost-bunsen-array-container
          bunsenId=renderId
          bunsenModel=subModel
          bunsenStore=bunsenStore
          cellConfig=mergedConfig
          errors=errors
          onChange=onChange
          readOnly=readOnly
          required=required
          value=value
        }}
      {{/if}}
    {{else}}
      {{frost-bunsen-input-wrapper
        bunsenId=renderId
        bunsenModel=subModel
        bunsenStore=bunsenStore
        cellConfig=mergedConfig
        errorMessage=errorMessage
        onChange=onChange
        readOnly=readOnly
        required=required
        isDependencyMet=isDependencyMet
        value=renderValue
      }}
    {{/if}}
  {{/if}}
  {{#each mergedConfig.children as |child index|}}
    {{frost-bunsen-cell
      bunsenId=bunsenId
      bunsenModel=bunsenModel
      bunsenStore=bunsenStore
      cellConfig=child
      errors=errors
      onChange=onChange
      readOnly=readOnly
      value=value
    }}
  {{/each}}
{{/if}}
